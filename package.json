{
  "name": "moggin",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "plugins:build": "tsc -p tsconfig.plugins.json",
    "plugins:bundle": "tsup modules/expo-glance-widget/plugins/index.ts --format cjs --out-dir modules/expo-glance-widget/plugins",
    "module:bundle": "tsup modules/expo-material-dynamic-colors/src/index.ts --format cjs --out-dir modules/expo-material-dynamic-colors/src/",
    "reset-project": "node ./scripts/reset-project.js",
    "remove-js-files": "tsx scripts/remove-js.ts modules/expo-glance-widget/plugins",
    "android": "expo run:android",
    "prebuild:android": "npm run plugins:bundle && expo prebuild -p android --clean",
    "build:android": "npm run prebuild:android && expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@legendapp/state": "3.0.0-beta.31",
    "@material/material-color-utilities": "^0.3.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "@shopify/react-native-skia": "2.0.0-next.4",
    "@tanstack/react-query": "^5.81.5",
    "@types/node": "^24.0.10",
    "@types/xml2js": "^0.4.14",
    "babel-plugin-module-resolver": "^5.0.2",
    "colors": "^1.4.0",
    "deepmerge": "^4.3.1",
    "expo": "53.0.19",
    "expo-blur": "~14.1.5",
    "expo-build-properties": "~0.14.6",
    "expo-constants": "~17.1.6",
    "expo-dev-client": "~5.2.1",
    "expo-font": "~13.3.1",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.3.0",
    "expo-linking": "~7.1.5",
    "expo-network": "^7.1.5",
    "expo-router": "~5.1.0",
    "expo-splash-screen": "~0.30.9",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.9",
    "expo-web-browser": "~14.2.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "~3.17.5",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5",
    "ts-node": "^10.9.2",
    "victory-native": "^41.17.4",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/color": "^4.2.0",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.0.10",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "jest": "~29.7.0",
    "ts-jest": "^29.4.0",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3"
  },
  "private": true
}
